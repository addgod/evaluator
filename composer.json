{
	"name": "elepunk/evaluator",
	"description": "Laravel package for symfony expression language",
	"license": "MIT",
	"homepage": "http://elepunk.com",
	"keywords": ["laravel", "orchestra", "expression-language"],
	"authors": [
		{
			"name": "Ahmad Shah Hafizan Hamidin",
			"email": "ahmadshahhafizan@gmail.com",
			"homepage": "https://github.com/ahmadshah"
		}
	],
	"require": {
		"php": ">=5.4",
		"illuminate/support": "5.*",
		"illuminate/contracts": "5.*",
		"symfony/expression-language": "~2.6"
	},
	"require-dev": {
	        "mockery/mockery": "~0.9",
	        "satooshi/php-coveralls": "~0.6"
	},
	"autoload": {
		"psr-4": {
			"Elepunk\\Evaluator\\": "src/"
		}
	},
	"suggest": {
        		"orchestra/memory": "Allow using orchestra/memory as a driver"
    	},
	"mininum-stability": "stable"
}